pragma solidity ^0.8.0;

contract SimpleContract {
    function add(uint z,uint y) external pure returns (uint){
        return  z + y;
    }
    function divide(uint z,uint y) external pure returns (uint){
        return  z / y;
    }
    function multiply(uint z,uint y) external pure returns (uint){
        return  z * y;
    }
    function subract(uint z,uint y) external pure returns (uint){
        return  z - y;
    }
}
